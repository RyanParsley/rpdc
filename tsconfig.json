{
  "extends": "astro/tsconfigs/strictest",
  "exclude": ["node_modules", "public", "dist"],
  "compilerOptions": {
    "lib": ["ESNext"],
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@data/*": ["data/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
